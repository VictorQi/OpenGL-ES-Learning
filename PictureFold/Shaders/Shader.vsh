/////////////////////////////////////////////////////////////////
// VERTEX ATTRIBUTES
/////////////////////////////////////////////////////////////////
attribute vec4 aPosition;
attribute vec2 aTextureCoord0;

/////////////////////////////////////////////////////////////////
// Varyings
/////////////////////////////////////////////////////////////////
varying lowp vec2 vTextureCoord0;

/////////////////////////////////////////////////////////////////
// UNIFORMS
/////////////////////////////////////////////////////////////////


void main()
{
    
    vTextureCoord0 = aTextureCoord0.st;
    
    gl_Position = aPosition;
}
